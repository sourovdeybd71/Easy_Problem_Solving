//Sum of a 5 digit number
#include<stdio.h>
int main(){
  int t, i, n, rem, sum=0;
  printf("Enter the integer: ");
  scanf("%d",&n);

  // t=n;
  while(n){
    rem = n%10;
    sum=sum+rem;
    n=n/10;

  }
  printf("%d", sum);
return 0;
}
